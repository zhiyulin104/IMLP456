    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "np.random.seed(1)\n",
    "x = np.random.randint(0, 100, 50)\n",
    "y = x + np.random.normal(0, 10, 50)\n",
    "\n",
    "coef = np.corrcoef(x, y)\n",
    "print(coef)\n",
    "plt.scatter(x, y)\n",
    "plt.show()"
   ]
